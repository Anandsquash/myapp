onents\\HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1>In HelloWorld Component</h1>\n    <h1>{{ msg }}</h1>\n    <span> Name :</span> <span>{{name}}</span> <br>\n    <span> Company :</span> <span> {{company}}</span> <br>\n    <span> Designation :</span><span> {{designation}}</span> <br>\n    <Button v-on:click=\"buttonClicked()\">Emit Button</Button>\n    <p>{{ message }} </p>\n    <a href=\"https://squashapps.com/\">SquashApps</a>\n        \n    <!-- <h2>{{nameF}}</h2> -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  data() {\n    return {\n      message: \"This is Binding Concept\"\n    }\n    \n  },\n  props: {\n    \n    name: String,\n    company:String,\n    designation:String,\n    msg: String,\n    \n  },\n  methods:{\n    buttonClicked(){\n      this.$emit(\"my-emit\",this.name)\n      // this.$emit(\"my-emit\",'JoAnand')\n    },\n    emitEvent(){\n      this.$emit(\"my-emit\",this.name)\n    }\n  },\n  mounted(){\n    this.emitEvent()\n    // this.buttonClicked()\n    // this.$emit(\"my-emit\",'mounted')\n  },\n  created(){\n    this.$emit(\"my-emit\",'created')\n  },\n  updated(){\n    this.$emit(\"my-emit\",'updated')\n  },\n  unmounted(){\n    this.$emit(\"my-emit\",'destroyed')\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n  .hello{\n    border:1px solid red;\n    background-color: aqua;\n  }\n  h1{\n    color:rgb(6, 6, 17);\n    font-size:large;\n  }\n</style>\n"]}]}